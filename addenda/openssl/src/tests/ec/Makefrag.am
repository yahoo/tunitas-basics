check_bin_ec = check/bin/ec
check_bin_ec_LDADD = \
  $(lib_libopenssl_module_la) \
  $(end)
check_bin_ec_LDFLAGS = $(nothing_more)
check_bin_ec_MODULES = $(call HGTW_SOURCEStoMODULES, $(check_bin_ec_SOURCES))
check_bin_ec_SOURCES = \
  obj/src/tests/ec/main.cpp \
  obj/src/tests/ec/namespace.cpp \
  $(end)
$(scold_cxxdir)/tests/ec/dependencies.mk: $(check_bin_ec_SOURCES)
$(check_bin_ec_SOURCES) : obj/src/%.cpp : src/%.xcpp
	$(HT_V_COLD) $(call HT_XCPP2CPP, $^)
	@test -e $@
$(scold_cxxdir)/tests/ec/dependencies.mk:
	$(HT_V_BOTH) $(call HT_DEPENDENCIES, $^)
	@test -e $@
include $(scold_cxxdir)/tests/ec/dependencies.mk #       automake deletes am--include-marker during make am--depfiles
am--depfiles: $(scold_cxxdir)/tests/ec/dependencies.mk # automake during configure
clean-dependencies::
	rm -f $(scold_cxxdir)/tests/ec/dependencies.mk
