usage parse [ -E | --either  ]
            [ -L | --long-only ]
            [ -S | --short-only ]
            [ -a | --apple ]
            [ -b | --banana ]
            [ -c | --cherry ]
            [ { -d | --dog } value ]       (--dog requires a value)
            [ { -e | --eagle } value ]     (--eagle requires a value)
            [ { -f | --frog } [ value ] ]  (--frog accepts an optional value)
            [ { -g | --goat } [ value ] ]  (--goat accepts an optional value)
            [ ...query... ]

query ::= { apple | banana | cherry | dog | eagle | frog | goat }
