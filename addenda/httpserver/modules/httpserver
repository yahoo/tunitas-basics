#pragma once
//
// Only <httpserver.hpp> or <httpserverpp> can be included directly.
//
// Observe the underlying release (libhttpserver-devel-0.9.0-3.fc24.x86_64)
//
//   #include <httpserver.hpp>      /usr/include/httpserver.hpp
//   #include <httpserverpp>        /usr/include/httpserverpp
//
//   /usr/include/httpserver <--------- a directory of other C++
//
// Observe the SCOLDification is basically the same thing
//
//   #import httpserver
//   #import httpserver.Webserver
//   ...etc...
//
#include <httpserver.hpp>
namespace httpserver { }
