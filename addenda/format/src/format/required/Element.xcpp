#divert <fpp>
#import std.ostream
namespace format::required {
  //
  // The minimal statement of an element object necessary for output formatting.
  //
  template<typename ELEMENT> concept Element = requires(ELEMENT const &element, std::ostream &outbound) {
    { outbound << element };
  };
}
#endiv
